<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
<!--        Tree view for Timesheet cost-->
        <record id="timesheet_cost_view_tree" model="ir.ui.view">
            <field name="name">timesheet.cost.view.tree</field>
            <field name="model">timesheet.cost</field>
            <field name="arch" type="xml">
                <tree string="timesheet_cost" create="0" import="false">
                    <field name="employee_id" column_invisible="True" />
                    <field name ="employee_name" column_invisible="True"/>
                    <field name="updated_date"/>
                    <field name="current_value"/>
                </tree>
            </field>
        </record>
</odoo>
